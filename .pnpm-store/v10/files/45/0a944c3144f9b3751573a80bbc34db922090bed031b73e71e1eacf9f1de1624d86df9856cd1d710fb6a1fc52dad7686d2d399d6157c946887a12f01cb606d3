export declare function validateQuantityInput({ balance, decimals }: {
    balance?: string | undefined;
    decimals?: number | undefined;
}): {
    insufficient: () => "Insufficient balance" | undefined;
    decimal: (value: string) => string | undefined;
    range: (value: string) => string | undefined;
};
export declare function validateSlippage(value: string): {
    type: "error";
    message: string;
} | {
    type: "warning";
    message: string;
} | {
    type: "success";
    message: string;
};
