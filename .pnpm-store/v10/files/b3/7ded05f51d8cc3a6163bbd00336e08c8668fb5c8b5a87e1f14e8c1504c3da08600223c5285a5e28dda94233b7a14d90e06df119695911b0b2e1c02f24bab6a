import { SvelteComponentTyped } from "svelte";
declare const __propDef: {
    props: {
        size?: number | undefined;
        color?: string | undefined;
    };
    events: {
        [evt: string]: CustomEvent<any>;
    };
    slots: {};
};
export type SadProps = typeof __propDef.props;
export type SadEvents = typeof __propDef.events;
export type SadSlots = typeof __propDef.slots;
export default class Sad extends SvelteComponentTyped<SadProps, SadEvents, SadSlots> {
}
export {};
