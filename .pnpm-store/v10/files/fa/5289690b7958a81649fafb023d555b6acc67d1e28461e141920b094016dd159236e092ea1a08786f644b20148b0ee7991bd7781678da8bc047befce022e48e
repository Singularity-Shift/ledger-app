import { JSONSerializable } from '../../../../util/json';
import { Params as Params_pb } from '@initia/initia.proto/ibc/core/connection/v1/connection';
export declare class IbcConnectionParams extends JSONSerializable<IbcConnectionParams.Amino, IbcConnectionParams.Data, IbcConnectionParams.Proto> {
    max_expected_time_per_block: number;
    constructor(max_expected_time_per_block: number);
    static fromAmino(data: IbcConnectionParams.Amino): IbcConnectionParams;
    toAmino(): IbcConnectionParams.Amino;
    static fromData(data: IbcConnectionParams.Data): IbcConnectionParams;
    toData(): IbcConnectionParams.Data;
    static fromProto(proto: IbcConnectionParams.Proto): IbcConnectionParams;
    toProto(): IbcConnectionParams.Proto;
}
export declare namespace IbcConnectionParams {
    interface Amino {
        max_expected_time_per_block: string;
    }
    interface Data {
        max_expected_time_per_block: string;
    }
    type Proto = Params_pb;
}
