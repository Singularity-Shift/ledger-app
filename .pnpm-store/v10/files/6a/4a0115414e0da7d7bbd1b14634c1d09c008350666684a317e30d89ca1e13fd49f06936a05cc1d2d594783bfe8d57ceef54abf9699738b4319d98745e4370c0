export interface CollectionInfo {
    creator?: string;
    name: string;
    description?: string;
    nfts?: {
        handle: string;
        length: string;
    };
    uri?: string;
}
export interface NFTTokenInfoBase {
    token_id: string;
    uri: string;
}
export interface NFTTokenInfoMove extends NFTTokenInfoBase {
    collection: {
        inner: string;
    };
    description: string;
}
declare function createInitiaNftClient(rest: string, collectionAddress: string, type: "minimove" | "miniwasm" | "minievm" | "custom"): {
    rest: string;
    getCollectionInfo: () => Promise<CollectionInfo>;
    getTokenInfo: (tokenAddress: string) => Promise<NFTTokenInfoMove | {
        token_id: string;
        uri: string;
    }>;
};
export default createInitiaNftClient;
