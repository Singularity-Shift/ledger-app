export declare function calcMinimumReceived(simulatedAmount: string | number): number;
export interface SlippageRange {
    available: {
        min: number;
        max: number;
    };
    recommended: {
        min: number;
        max: number;
    };
}
export declare function validateSlippageInput(input: string, range?: SlippageRange): {
    type: "error";
    message: string;
} | {
    type: "warning";
    message: string;
} | {
    type: "success";
    message: string;
};
export declare function sanitizeInput(decimalPlaces?: number): (e: Event) => void;
