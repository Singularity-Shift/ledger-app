import { SvelteComponentTyped } from "svelte";
declare const __propDef: {
    props: {
        size?: number | undefined;
        color?: string | undefined;
    };
    events: {
        [evt: string]: CustomEvent<any>;
    };
    slots: {};
};
export type DragHandleProps = typeof __propDef.props;
export type DragHandleEvents = typeof __propDef.events;
export type DragHandleSlots = typeof __propDef.slots;
export default class DragHandle extends SvelteComponentTyped<DragHandleProps, DragHandleEvents, DragHandleSlots> {
}
export {};
