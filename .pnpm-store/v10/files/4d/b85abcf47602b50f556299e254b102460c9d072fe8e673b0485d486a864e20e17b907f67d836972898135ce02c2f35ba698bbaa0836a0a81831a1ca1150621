import { Asset, Chain } from '@initia/initia-registry-types';
export declare function ensureValidURL(url: string): string;
export declare function getName(layer: Chain): string;
export declare function getRPC(layer: Chain): string;
export declare function getRest(layer: Chain): string;
export declare function getLogo(item: {
    logo_URIs?: {
        png?: string;
        svg?: string;
    };
}): string | undefined;
export declare function getDecimals(asset: Asset): number;
export declare function findChannel(sourceChain: Chain, targetChain: Chain, type: "transfer" | "nft-transfer"): {
    chain_id: string;
    port_id: string;
    channel_id: string;
    version: string;
} | undefined;
export declare function findTransferChannelId(sourceChain: Chain, targetChain: Chain): string | undefined;
export declare function findNftTransferChannel(sourceChain: Chain, targetChain: Chain): {
    chain_id: string;
    port_id: string;
    channel_id: string;
    version: string;
} | undefined;
