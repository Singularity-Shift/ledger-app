    authority: AccAddress;
    bridge_id: number;
    metadata: string;
    constructor(authority: AccAddress, bridge_id: number, metadata: string);
    static fromAmino(data: MsgUpdateMetadata.Amino): MsgUpdateMetadata;
    toAmino(): MsgUpdateMetadata.Amino;
    static fromData(data: MsgUpdateMetadata.Data): MsgUpdateMetadata;
    toData(): MsgUpdateMetadata.Data;
    static fromProto(data: MsgUpdateMetadata.Proto): MsgUpdateMetadata;
    toProto(): MsgUpdateMetadata.Proto;
    packAny(): Any;
    static unpackAny(msgAny: Any): MsgUpdateMetadata;
}
export declare namespace MsgUpdateMetadata {
    interface Amino {
        type: 'ophost/MsgUpdateMetadata';
        value: {
            authority: AccAddress;
