import { JSONSerializable } from '../../util/json';
import { Params as Params_pb } from '@initia/initia.proto/cosmos/auth/v1beta1/auth';
export declare class AuthParams extends JSONSerializable<AuthParams.Amino, AuthParams.Data, AuthParams.Proto> {
    max_memo_characters: number;
    tx_sig_limit: number;
    tx_size_cost_per_byte: number;
    sig_verify_cost_ed25519: number;
    sig_verify_cost_secp256k1: number;
    constructor(max_memo_characters: number, tx_sig_limit: number, tx_size_cost_per_byte: number, sig_verify_cost_ed25519: number, sig_verify_cost_secp256k1: number);
    static fromAmino(data: AuthParams.Amino): AuthParams;
    toAmino(): AuthParams.Amino;
    static fromData(data: AuthParams.Data): AuthParams;
    toData(): AuthParams.Data;
    static fromProto(data: AuthParams.Proto): AuthParams;
    toProto(): AuthParams.Proto;
}
export declare namespace AuthParams {
    interface Amino {
        type: 'cosmos-sdk/x/auth/Params';
        value: {
            max_memo_characters: string;
            tx_sig_limit: string;
            tx_size_cost_per_byte: string;
            sig_verify_cost_ed25519: string;
            sig_verify_cost_secp256k1: string;
        };
    }
    interface Data {
        '@type': '/cosmos.auth.v1beta1.Params';
        max_memo_characters: string;
        tx_sig_limit: string;
        tx_size_cost_per_byte: string;
        sig_verify_cost_ed25519: string;
        sig_verify_cost_secp256k1: string;
    }
    type Proto = Params_pb;
}
