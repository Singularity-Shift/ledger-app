import { BehaviorSubject } from 'rxjs';
import { SkipChain, SkipAsset } from '../../lib/bridge-sdk';
export interface FormValues {
    srcChain: SkipChain | null;
    srcAsset: SkipAsset | null;
    dstChain: SkipChain | null;
    dstAsset: SkipAsset | null;
    quantity: string;
    slippage: number;
    transferMethod?: "OP" | "IBC SWAP";
}
export declare const initialFormValues: FormValues;
interface FormError {
    message?: string;
    buttonMessage: string;
    topMessage?: string;
}
export declare const formValues$: BehaviorSubject<FormValues>;
export declare const setFormValues: (values: Partial<FormValues>) => void;
export declare const parseQuantity: (input: string) => string;
export declare const getFormError$: BehaviorSubject<(srcBalance: string | undefined, simulationError: Error | null) => FormError | null>;
export {};
