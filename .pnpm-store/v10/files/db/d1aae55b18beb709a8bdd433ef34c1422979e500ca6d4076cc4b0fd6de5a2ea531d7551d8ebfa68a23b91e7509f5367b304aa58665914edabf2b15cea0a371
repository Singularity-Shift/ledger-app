export interface BridgeResponse {
    bridge_id: string;
    bridge_addr: string;
    bridge_config: {
        challengers: string[];
        proposer: string;
        batch_info: {
            submitter: string;
            chain: string;
        };
        submission_interval: string;
        finalization_period: string;
        submission_start_time: Date;
        metadata: string;
    };
}
export declare function getOpWithdrawalTime(bridgeId: string): Promise<string>;
