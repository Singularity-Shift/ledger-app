import { JSONSerializable } from '../../util/json';
import { AccAddress } from '../bech32';
import { ACL as ACL_pb } from '@initia/initia.proto/initia/ibchooks/v1/types';
export declare class ACL extends JSONSerializable<ACL.Amino, ACL.Data, ACL.Proto> {
    address: AccAddress;
    allowed: boolean;
    constructor(address: AccAddress, allowed: boolean);
    static fromAmino(data: ACL.Amino): ACL;
    toAmino(): ACL.Amino;
    static fromData(data: ACL.Data): ACL;
    toData(): ACL.Data;
    static fromProto(data: ACL.Proto): ACL;
    toProto(): ACL.Proto;
}
export declare namespace ACL {
    interface Amino {
        address: AccAddress;
        allowed: boolean;
    }
    interface Data {
        address: AccAddress;
        allowed: boolean;
    }
    type Proto = ACL_pb;
}
