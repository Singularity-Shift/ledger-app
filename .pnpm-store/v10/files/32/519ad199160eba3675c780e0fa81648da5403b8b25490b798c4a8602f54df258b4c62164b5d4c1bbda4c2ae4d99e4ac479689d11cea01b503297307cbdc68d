import { JSONSerializable } from '../../util/json';
import { Denom } from '../Denom';
import { RewardWeight as RewardWeight_pb } from '@initia/initia.proto/initia/distribution/v1/distribution';
export declare class RewardWeight extends JSONSerializable<RewardWeight.Amino, RewardWeight.Data, RewardWeight.Proto> {
    denom: Denom;
    weight: string;
    constructor(denom: Denom, weight: string);
    static fromAmino(data: RewardWeight.Amino): RewardWeight;
    toAmino(): RewardWeight.Amino;
    static fromData(data: RewardWeight.Data): RewardWeight;
    toData(): RewardWeight.Data;
    static fromProto(data: RewardWeight.Proto): RewardWeight;
    toProto(): RewardWeight.Proto;
}
export declare namespace RewardWeight {
    interface Amino {
        denom: Denom;
        weight: string;
    }
    interface Data {
        denom: Denom;
        weight: string;
    }
    type Proto = RewardWeight_pb;
}
